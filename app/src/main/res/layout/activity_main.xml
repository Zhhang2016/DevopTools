<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wxq.developtools.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
    />

    <ImageView
        android:id="@+id/iv_test_pic"
        android:layout_centerInParent="true"
        android:layout_width="200dp"
        android:layout_height="200dp" />




    <com.juziwl.uilibrary.niceplayer.NiceVideoPlayer
        android:id="@+id/player"
        android:layout_centerInParent="true"
        android:layout_width="match_parent"
        android:layout_height="200dp"></com.juziwl.uilibrary.niceplayer.NiceVideoPlayer>



    <com.juziwl.uilibrary.X5utils.X5WebView
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">




    </com.juziwl.uilibrary.X5utils.X5WebView>




</RelativeLayout>
